     :audio "/opt/homebrew/Cellar/switchaudio-osx/1.1.0/bin/SwitchAudioSource -t input -s '%s'
             /opt/homebrew/Cellar/switchaudio-osx/1.1.0/bin/SwitchAudioSource -t output -s '%s'"
